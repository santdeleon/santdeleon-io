{
  "name": "santdeleon-io",
  "version": "2.6.0",
  "private": false,
  "author": "Santiago Deleon",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "12.6.0",
    "@types/jest": "^26.0.23",
    "@types/moment": "^2.13.0",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "axios-hooks": "^2.6.3",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react-error-boundary": "^3.1.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.0",
    "styled-media-query": "^2.1.2",
    "typescript": "^4.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --ci --coverage --coverageDirectory=output/coverage/jest --reporters=default --reporters=jest-junit --testFailureExitCode=0 --silent"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.js'",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest-junit": {
    "outputDirectory": "./output/coverage/juint",
    "suiteNameTemplate": "{filepath}"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
